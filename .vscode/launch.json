    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/vim",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [{"name":"DYLD_LIBRARY_PATH", "value":"./"},
                {"name":"DYLD_INSERT_LIBRARIES", "value":"libft_malloc.so"},
                {"name":"DYLD_FORCE_FLAT_NAMESPACE", "value":"1"}],
            "externalConsole": true,
            "MIMode": "lldb"
        }
    ]
}